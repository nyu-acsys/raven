{
  "name": "tree-sitter-raven",
  "version": "0.1.0",
  "description": "A treesitter (syntax highlight) grammar for raven",
  "main": "bindings/node",
  "types": "bindings/node",
  "repository": null,
  "author": "Jacob Salzberg",
  "license": null,
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp": "^10.2.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.0"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "tree-sitter": [
    {
      "scope": "source.raven",
      "highlights": "queries/highlights.scm",
      "file-types": [
        "rav"
      ]
    }
  ]
}
