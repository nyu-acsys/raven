interface Type {
  rep type T
}

module Pair[T1: Type, T2: Type] {
  rep type T = data {
    case pair(fst: T1, snd: T2)
  }
}

module Test {
  module IntT: Type {
    rep type T = Int
  }

  module BoolT: Type {
    rep type T = Bool
  }

  module P = Pair[IntT, BoolT]

  val q: P = P.pair(1, true)

  module P2 = Pair[BoolT, P]

  val p: P2 = P2.pair(true, P.pair(1, true))

  val x: Bool = p.P2.fst

}
